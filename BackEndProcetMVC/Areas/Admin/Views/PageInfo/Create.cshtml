<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>

@model PageInfo

<form method="post" style="margin:10px;" enctype="multipart/form-data">
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Adress</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="adress.." asp-for="@Model.Adress">
        <span class="alert alert-text" asp-validation-for="@Model.Adress" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">phone number</label>
        <input type="tel" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="number.." asp-for="@Model.PhoneNumber">
        <span class="alert alert-text" asp-validation-for="@Model.PhoneNumber" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Email</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="email.." asp-for="@Model.Email">
        <span class="alert alert-text" asp-validation-for="@Model.Email" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Instagram</label>
        <input  class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="instagram.." asp-for="@Model.Instagram">
        <span class="alert alert-text" asp-validation-for="@Model.Instagram" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Facebook</label>
        <input class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="facebook.." asp-for="@Model.Facebook">
        <span class="alert alert-text" asp-validation-for="@Model.Facebook" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">linkedin</label>
        <input class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="linkedin.." asp-for="@Model.Linkedin">
        <span class="alert alert-text" asp-validation-for="@Model.Linkedin" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Twitter</label>
        <input class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="linkedin.." asp-for="@Model.Twitter">
        <span class="alert alert-text" asp-validation-for="@Model.Twitter" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Detail</label>
        <input class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="detail.." asp-for="@Model.Detail">
        <span class="alert alert-text" asp-validation-for="@Model.Detail" style="color:red; font-size:16px;"></span>
    </div>


    <button type="submit" class="btn btn-primary">Submit</button>
</form>

