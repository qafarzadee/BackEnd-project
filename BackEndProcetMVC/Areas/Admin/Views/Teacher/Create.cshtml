<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>

@model Teacher

<form method="post" style="margin:10px;" enctype="multipart/form-data">
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Name</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="FullName.." asp-for="@Model.Name">
        <span class="alert alert-text" asp-validation-for="@Model.Name" style="color:red; font-size:16px;"></span>
    </div>

    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Degree</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="degree.." asp-for="@Model.Degree">
        <span class="alert alert-text" asp-validation-for="@Model.Degree" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Email</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="email.." asp-for="@Model.Email">
        <span class="alert alert-text" asp-validation-for="@Model.Email" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">FacebookLink</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Facebook.." asp-for="@Model.FaceBookLink">
        <span class="alert alert-text" asp-validation-for="@Model.Email" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Instagram</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="insatagram.." asp-for="@Model.InstagramLink">
        <span class="alert alert-text" asp-validation-for="@Model.Email" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Twitter</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="twitter.." asp-for="@Model.LinkedinLink">
        <span class="alert alert-text" asp-validation-for="@Model.LinkedinLink" style="color:red; font-size:16px;"></span>
    </div>
       <div class="mb-3">
     <label for="exampleInputEmail1" class="form-label">language Percent</label>
        <input  class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="language.." asp-for="@Model.LanguagePercent">
        <span class="alert alert-text" asp-validation-for="@Model.LanguagePercent" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">team leader percent</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="teamleader.." asp-for="@Model.TeamLeaderPercent">
        <span class="alert alert-text" asp-validation-for="@Model.TeamLeaderPercent" style="color:red; font-size:16px;"></span>
    </div>
       <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Development</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="development.." asp-for="@Model.DevelopmentPercent">
        <span class="alert alert-text" asp-validation-for="@Model.DevelopmentPercent" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Design Percent</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="design percent.." asp-for="@Model.DesignPercent">
        <span class="alert alert-text" asp-validation-for="@Model.DesignPercent" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Innovation percent</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Innovation percent.." asp-for="@Model.InnovationPercent">
        <span class="alert alert-text" asp-validation-for="@Model.InnovationPercent" style="color:red; font-size:16px;"></span>
    </div>
           <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Communucation Percent</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="communucation.." asp-for="@Model.CommunicationPercent">
        <span class="alert alert-text" asp-validation-for="@Model.CommunicationPercent" style="color:red; font-size:16px;"></span>
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Image</label>
        <input  class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="image.." asp-for="@Model.formFile">
        <span class="alert alert-text" asp-validation-for="@Model.formFile" style="color:red; font-size:16px;"></span>
    </div>
       <div class="mb-3">
     <label for="exampleInputEmail1" class="form-label">Phone</label>
        <input type="tel" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="twitter.." asp-for="@Model.Phone">
        <span class="alert alert-text" asp-validation-for="@Model.Phone" style="color:red; font-size:16px;"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

