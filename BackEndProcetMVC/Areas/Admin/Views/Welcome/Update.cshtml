@model Welcome

<form method="post" style="margin:10px;" enctype="multipart/form-data">
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Titte</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="tittle.." asp-for="@Model.Tittle">
        <span class="alert alert-text" asp-validation-for="@Model.Tittle" style="color:red; font-size:16px;"></span>
    </div>

    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Description</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="tittle.." asp-for="@Model.Description">
        <span class="alert alert-text" asp-validation-for="@Model.Description" style="color:red; font-size:16px;"></span>
    </div>

    <div class="mb-3">
        <label asp-for="@Model.Formfile" class="form-label"></label>
        <input type="file" class="form-control" id="imageInput" asp-for="Formfile">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>

